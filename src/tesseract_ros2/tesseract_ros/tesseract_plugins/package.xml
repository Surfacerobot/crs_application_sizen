<?xml version="1.0"?>
<package format="2">
  <name>tesseract_plugins</name>
  <version>0.1.0</version>
  <description>Tesseract ROS Plugins</description>
  <maintainer email="levi.armstrong@swri.org">Levi Armstrong</maintainer>
  <license>Apache 2.0</license>
  <license>BSD</license>
  <author>Levi Armstrong</author>
  <author>John Schulman</author>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>tf2_eigen</depend>
  <depend>tesseract_msgs</depend>
  <depend>tesseract_collision</depend>
  <depend>pluginlib</depend>

  <depend>eigen</depend>

  <!-- The export tag contains other, unspecified, tags -->
  <export>
      <build_type>ament_cmake</build_type>
      <tesseract_collision plugin="${prefix}/tesseract_collision_bullet_plugin_description.xml"/>
      <tesseract_collision plugin="${prefix}/tesseract_collision_fcl_plugin_description.xml"/>
  </export>
</package>
